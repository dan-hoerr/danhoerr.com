<div class="wrapper">

<h1>Listing users</h1>
<%= link_to 'New User', new_user_path %>
<table>
  <tr>
    <th>User</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
</div>
