<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Title</th>
			<th>MultiverseID</th>
			<th>Text</th>
		</tr>
	</thead>
	<tbody>
<% @planes.each do |plane| %>
  <tr>
  	<td><%= plane.id %></td>
    <td><%= plane.title %></td>
    <td><%= plane.multiverseid %></td>
    <td><%=raw plane.text %></td>
  </tr>

<% end %>
</tbody>
</table>